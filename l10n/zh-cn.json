	"webviews": {
		"_comment": "Webview 界面文本翻译 - 支持正则匹配",

		"patterns": [
			{
				"match": "GitLens is now installed in (.+)",
				"replace": "GitLens 已安装到 $1"
			},
			{
				"match": "Understand every line of code — instantly",
				"replace": "即时理解每一行代码"
			},
			{
				"match": "With <span class=\"accent\">PRO</span> subscription you get more",
				"replace": "订阅 <span class=\"accent\">PRO</span> 版本可获得更多功能"
			},
			{
				"match": "Blame Annotations & Hovers",
				"replace": "代码归属注释与悬停提示"
			},
			{
				"match": "See who changed what and why",
				"replace": "查看谁在何时修改了什么及原因"
			},
			{
				"match": "Learn more about inline blame",
				"replace": "了解更多关于内联代码归属"
			},
			{
				"match": "Configure autolinks",
				"replace": "配置自动链接"
			},
			{
				"match": "GitLens Inspect",
				"replace": "GitLens 检查"
			},
